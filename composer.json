{
    "name": "wieni/wmmodel_factory",
    "type": "drupal-module",
    "description": "Define factories that allow you to generate entities with certain fields, filled with random data",
    "license": "MIT",
    "authors": [
        {
            "name": "Wieni",
            "email": "info@wieni.be"
        }
    ],
    "require": {
        "php": "^7.3 || ^8.0",
        "drupal/core": "^9.3 || ^10",
        "fakerphp/faker": "^1.10"
    },
    "require-dev": {
        "ergebnis/composer-normalize": "^2.0",
        "wieni/wmcodestyle": "^1.3"
    },
    "scripts": {
        "coding-standards": [
            "@composer normalize",
            "php-cs-fixer fix --config=.php_cs.php",
            "phpstan analyse"
        ]
    }
}
